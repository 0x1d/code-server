{
	"compilerOptions": {
		"target": "esnext",
		"module": "commonjs",
		"baseUrl": ".",
		"rootDir": ".",
		"jsx": "react",
		"outDir": "out",
		"declaration": false,
		"sourceMap": true,
		"strict": true,
		"resolveJsonModule": true,
		"experimentalDecorators": true,
		"importHelpers": true,
		"plugins": [{
			"name": "typescript-tslint-plugin"
		}],
		"paths": {
			"@coder/*": [
				"packages/*/src"
			],
			"vs/*": [
				"lib/vscode/src/vs/*"
			]
		}
	}
}
